{
  "name": "website-template",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "qa:stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "qa:stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "qa:prettier": "prettier --check .",
    "qa:prettier:fix": "prettier --write .",
    "qa:eslint": "eslint .",
    "qa:eslint:fix": "eslint . --fix",
    "qa": "yarn qa:stylelint && yarn qa:prettier && yarn qa:eslint",
    "qa:fix": "yarn qa:stylelint:fix && yarn qa:prettier:fix && yarn qa:eslint:fix",
    "prepare": "husky",
    "generate:migration:hygraph": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileUrl } from \"node:url\"; register(\"ts-node/esm\", pathToFileUrl(\"./\"));' src/lib/hygraph/generate-migration.ts",
    "push:migration:hygraph": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileUrl } from \"node:url\"; register(\"ts-node/esm\", pathToFileUrl(\"./\"));' src/lib/hygraph/migration-runner.ts",
    "pull:graphql-schema": "npx graphql-codegen --config codegen.ts",
    "generate:typed-graphql": "npx typed-graphql-builder --schema ./src/lib/hygraph/graphql/schema.graphql --output ./src/lib/graphql-api.ts",
    "migrate:db": "drizzle-kit push",
    "migrate:hygraph": "yarn push:migration:hygraph && yarn pull:graphql-schema && yarn generate:typed-graphql"
  },
  "dependencies": {
    "@astrojs/node": "^9.2.2",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.4.1",
    "@libsql/client": "^0.15.9",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.8",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.9.2",
    "astro-capo": "^0.0.1",
    "astro-compress": "^2.3.8",
    "astro-icon": "^1.1.5",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4",
    "astro-seo-schema": "^5.0.0",
    "better-auth": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.2",
    "lucide-react": "^0.513.0",
    "p-throttle": "^7.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.89.2",
    "schema-dts": "^1.1.5",
    "shadcn": "^2.6.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8",
    "tw-animate-css": "^1.3.4",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/css": "^0.8.1",
    "@eslint/js": "^9.28.0",
    "@eslint/json": "^0.12.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-tools/url-loader": "^8.0.31",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@hygraph/management-sdk": "^1.4.0",
    "@iconify-json/lucide": "^1.2.47",
    "@typescript-eslint/parser": "^8.33.1",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.28.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "stylelint": "^16.20.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^38.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite-plugin-eslint": "^1.8.1"
  },
  "license": "MIT",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
